<section id="intro" class="intro-section pb-2" data-animate="slideInUp">
  <div class="container text-center" style="margin-top:50px">
    <h4>Add Project</h4>
    <form method="post" action="/admin/add" enctype="multipart/form-data">
      <div class="form-group">
        <input type="text" class="form-control" name="title" placeholder="Title">
      </div>
      <div class="form-group">
        <input type="text" class="form-control" name="service" placeholder="Service">
      </div>
      <div class="form-group">
        <input type="text" class="form-control" name="client" placeholder="Client">
      </div>
      <div class="form-group">
        <textarea class="form-control" name="description" placeholder="Description"></textarea>
      </div>
      <div class="form-group">
        <input type="text" class="form-control" name="url" placeholder="Project URL">
      </div>
      <div class="form-group">
        <input type="date" class="form-control" name="projectdate" placeholder="Project Date">
      </div>
      <div class="form-group">
        <input type="file" class="form-control" name="projectimage" placeholder="Project Image">
      </div>
      <button type="submit" class="button">Submit</button>
    </form>
  </div>
</section>